{
  "name": "reloc",
  "version": "2.0.7",
  "description": "React logic (control statements) components",
  "author": "TuyenNV <tuyennv.se@gmail.com>",
  "keywords": [
    "jsx",
    "react",
    "reactjs",
    "react-components",
    "conditional",
    "control-flow",
    "logic",
    "if",
    "switch",
    "for",
    "conditional-rendering",
    "logic-components",
    "headless-components",
    "declarative"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "sideEffects": false,
  "homepage": "https://github.com/biani-dev/reloc",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biani-dev/reloc.git"
  },
  "scripts": {
    "build:esm": "esbuild src/index.js --bundle --minify --format=esm --target=es2018 --jsx-factory=React.createElement --jsx-fragment=React.Fragment --external:react --outfile=dist/index.js",
    "build:cjs": "esbuild src/index.js --bundle --minify --format=cjs --target=es2018 --jsx-factory=React.createElement --jsx-fragment=React.Fragment --external:react --outfile=dist/index.cjs.js",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "test": "yarn build && jest",
    "update": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.27.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x || ^19.x"
  },
  "dependencies": {}
}
